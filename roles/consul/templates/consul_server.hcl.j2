server = true
bootstrap_expect = {{ consul_bootstrap_expect }}
datacenter = "dc1"

addresses {
  grpc  = "0.0.0.0"
  https = "0.0.0.0"
}

tls {
  ca_file = "/etc/ssl/consul_ca.crt"
  cert_file = "/etc/ssl/consul.crt"
  key_file = "/etc/ssl/consul.key"
  verify_incoming = true
  verify_outgoing = true
}

connect {
  enabled = true
}
